{
  "name": "@sweet-order/web-user-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "staging": "NODE_ENV=staging tsx watch src/index.ts",
    "production": "NODE_ENV=production tsx watch src/index.ts",
    "build:dev": "NODE_ENV=development tsc",
    "build:staging": "NODE_ENV=staging tsc",
    "build:production": "NODE_ENV=production tsc",
    "serve:dev": "NODE_ENV=development node dist/index.js",
    "serve:staging": "NODE_ENV=staging node dist/index.js",
    "serve:production": "NODE_ENV=production node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.5.0"
  }
}
